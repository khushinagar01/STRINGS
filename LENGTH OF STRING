#include<iostream>
using namespace std;
int findLength(char ch[],int size)
{
int length=0;
for(int i=0;i<size;i++)
    {
        if(ch[i]=='\0')
        {
            break;  //ruk jao
        }
        else{
            length++;
        }
    }
    return length;
}
int main()
{
    char ch[100];
    cin>>ch;
    int length=findLength(ch,100);
    
    cout<<"Length of string is : "<<length<<endl;
    return 0;
}
